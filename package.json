{
   "name": "sponsorhub",
   "version": "0.0.0",
   "packageManager": "bun@1.0.10",
   "description": "Toolkit for generating sponsors images",
   "author": "Dennis Ollhoff <contact@nyxb.email>",
   "license": "MIT",
   "funding": "https://github.com/sponsors/nyxb",
   "homepage": "https://github.com/nyxb/sponsorhub#readme",
   "repository": {
     "type": "git",
     "url": "git+https://github.com/nyxb/sponsorhub.git"
   },
   "bugs": {
     "url": "https://github.com/nyxb/sponsorhub/issues"
   },
   "keywords": [
     "sponsors",
     "github-sponsors"
   ],
   "workspaces": ["example"],
   "sideEffects": false,
   "exports": {
     ".": {
       "types": "./dist/index.d.ts",
       "import": "./dist/index.mjs",
       "require": "./dist/index.cjs"
     }
   },
   "main": "./dist/index.cjs",
   "module": "./dist/index.mjs",
   "types": "./dist/index.d.ts",
   "bin": {
     "sponsorhub": "./bin/sponsorhub.js"
   },
   "files": [
     "dist",
     "bin"
   ],
   "scripts": {
     "build": "unbuild",
     "stub": "unbuild --stub",
     "dev": "esno src/cli.ts",
     "lint": "eslint .",
     "prepublishOnly": "nyxr build",
     "release": "bumping --commit --push --tag && pnpm publish"
   },
   "dependencies": {
     "consola": "^3.2.3",
     "dotenv": "^16.3.1",
     "fs-extra": "^11.1.1",
     "image-data-uri": "^2.0.1",
     "node-html-parser": "^6.1.10",
     "ofetch": "^1.3.3",
     "picocolors": "^1.0.0",
     "sharp": "^0.32.5",
     "unconfig": "^0.3.10",
     "yargs": "^17.7.2"
   },
   "devDependencies": {
     "@nyxb/eslint-config": "^1.0.0-beta.15",
     "@nyxb/nyxi": "^0.0.31",
     "@nyxb/utils": "^0.0.3",
     "@types/fs-extra": "^11.0.2",
     "@types/node": "^20.6.2",
     "@types/yargs": "^17.0.24",
     "bumping": "^9.2.1",
     "eslint": "^8.53.0",
     "esno": "^0.17.0",
     "jiti": "^1.20.0",
     "typescript": "^5.2.2",
     "unbuild": "^2.0.0",
     "vite": "^4.4.9",
     "vitest": "^0.34.4"
   }
 }
 